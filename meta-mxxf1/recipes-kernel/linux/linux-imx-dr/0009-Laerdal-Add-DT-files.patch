+/dts-v1/;
+
+#include "imx6dl.dtsi"
+#include <dt-bindings/input/input.h>
+#include <dt-bindings/gpio/gpio.h>
+
+/ {
+    model = "Laerdal Medical SIMPAD PLUS (REV C)";
+    compatible = "datarespons,imx6", "fsl,imx6dl";
+
+    aliases {
+        mmc0 = &usdhc3;
+        mmc1 = &usdhc4;
+        rtc0 = &main_rtc;
+    };
+
+    memory {
+        reg = <0x10000000 0x40000000>;
+    };
+
+    regulators {
+        compatible = "simple-bus";
+
+        reg_5v: ext_5v {
+            compatible = "regulator-fixed";
+            regulator-name = "reg-5V";
+            regulator-min-microvolt = <5000000>;
+            regulator-max-microvolt = <5000000>;
+            gpio = <&gpio4 9 0>;
+            enable-active-high;
+            regulator-always-on;
+        };
+
+        reg_wifi: wifi_pwr {
+            compatible = "regulator-fixed";
+            regulator-name = "reg_wifi";
+            regulator-min-microvolt = <3300000>;
+            regulator-max-microvolt = <3300000>;
+            gpio = <&gpio3 26 0>;
+            enable-active-high;
+            regulator-always-on;
+        };
+
+        reg_wifi_io: wifi_pwr_io {
+            compatible = "regulator-fixed";
+            regulator-name = "reg_wifi_io";
+            regulator-min-microvolt = <1800000>;
+            regulator-max-microvolt = <1800000>;
+            gpio = <&gpio6 11 0>;
+            enable-active-high;
+            vin-supply = <&reg_wifi>;
+            startup-delay-us = <0>;
+            regulator-always-on;
+        };
+
+        reg_wifi_en: wifi_enable {
+            compatible = "regulator-fixed";
+            regulator-name = "reg_wifi_enable";
+            regulator-min-microvolt = <1800000>;
+            regulator-max-microvolt = <1800000>;
+            gpio = <&gpio2 3 0>;
+            enable-active-high;
+            vin-supply = <&reg_wifi_io>;
+            startup-delay-us = <300>;
+            regulator-always-on;
+        };
+
+/*
+        reg_bt_en: bt_enable {
+            compatible = "regulator-fixed";
+            regulator-name = "reg_bt_enable";
+            regulator-min-microvolt = <1800000>;
+            regulator-max-microvolt = <1800000>;
+            gpio = <&gpio2 4 0>;
+            enable-active-high;
+            vin-supply = <&reg_wifi_io>;
+            startup-delay-us = <300>;
+            regulator-always-on;
+        };
+*/
+
+        reg_usb_otg_vbus: usb_otg_vbus {
+            compatible = "regulator-fixed";
+            regulator-name = "usb_otg_vbus";
+            regulator-min-microvolt = <5000000>;
+            regulator-max-microvolt = <5000000>;
+            gpio = <&gpio3 22 0>;
+            enable-active-high;
+            vin-supply = <&reg_5v>;
+        };
+    };
+
+    gpio-keys {
+        compatible = "gpio-keys";
+        power {
+            label = "Power Button";
+            gpios = <&gpio3 29 GPIO_ACTIVE_LOW>;
+            linux,code = <KEY_POWER>; /* KEY_POWER */
+            gpio-key,wakeup;
+        };
+
+        key-function {
+            label = "Home";
+            gpios = <&gpio1 4 GPIO_ACTIVE_LOW>;
+            linux,code = <KEY_F12>;
+        };
+
+    };
+
+    bl0: backlight  {
+        compatible = "pwm-backlight";
+        pwms = <&pwm1 0 25000>;
+        brightness-levels = < 0 1 2 3 4 5 6 8  11 16  23  32 48
+                              64 81 100 121 144 169 196 225
+                              256 289 324 361 400 441 484 529
+                              576 625 676 729 784 841 900 961
+                              1024 1089 1156 1225 1296 1369 1444 1521
+                              1600 1681 1764 1849 1936 2025 2116 2209 >;
+        default-brightness-level = <32>;
+        power-supply = <&reg_5v>;
+        enable-gpios = <&gpio4 8 GPIO_ACTIVE_HIGH>;
+        status = "okay";
+    };
+
+	lcd_display: display@di0 {
+		compatible = "fsl,imx-parallel-display";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		interface-pix-fmt = "bgr666";
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_ipu1_simpad>;
+		status = "okay";
+
+		port@0 {
+			reg = <0>;
+
+			display0_in: endpoint {
+				remote-endpoint = <&ipu1_di0_disp0>;
+			};
+		};
+
+		port@1 {
+			reg = <1>;
+
+			lcd_display_out: endpoint {
+				remote-endpoint = <&lcd_panel_in>;
+			};
+		};
+		/*
+		display-timings {
+			native-mode = <&vga>;
+
+			vga: VGA {
+				clock-frequency = <25200000>;
+				hactive = <640>;
+				vactive = <480>;
+				hback-porch = <48>;
+				hsync-len = <96>;
+				hfront-porch = <16>;
+				vback-porch = <31>;
+				vsync-len = <2>;
+				vfront-porch = <12>;
+				hsync-active = <0>;
+				vsync-active = <0>;
+				de-active = <1>;
+				pixelclk-active = <0>;
+			};
+		};
+		*/
+	};
+
+	lcd_panel {
+		compatible = "edt,et057090dhu";
+		backlight = <&bl0>;
+		enable-gpios = <&gpio1 30 GPIO_ACTIVE_LOW>;
+		port {
+			lcd_panel_in: endpoint {
+				remote-endpoint = <&lcd_display_out>;
+			};
+		};
+	};
+
+
+    user-gpios {
+	/*
+        gpio-lcd-enable {
+            gpios = <&gpio1 30 GPIO_ACTIVE_LOW>;
+            status = "okay";
+            value = <0>;
+        };
+	*/
+        gpio-lcd-flip-h {
+            gpios = <&gpio1 7 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <0>;
+        };
+
+        gpio-lcd-flip-v {
+            gpios = <&gpio1 8 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <1>;
+        };
+
+        /*
+        gpio-wifi-enable {
+            gpios = <&gpio2 3 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <0>;
+        };
+		*/
+        gpio-bt-enable {
+            gpios = <&gpio2 4 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <1>;
+        };
+
+        gpio-spinor-wp {
+            gpios = <&gpio3 20 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <1>;
+        };
+
+        gpio-phy-rst {
+            gpios = <&gpio1 25 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <1>;
+        };
+
+        gpio-touch-rst {
+            gpios = <&gpio4 10 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <1>;
+        };
+
+        gpio-wlan-dev-wake {
+            gpios = <&gpio2 7 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <1>;
+        };
+
+        gpio-wlan-host-wake {
+            gpios = <&gpio2 6 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            bidir;
+        };
+
+        gpio-bt-host-wake {
+            gpios = <&gpio6 7 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            bidir;
+        };
+
+        gpio-bt-dev-wake {
+            gpios = <&gpio6 8 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            value = <1>;
+        };
+
+        gpio-bt-clk_req {
+            gpios = <&gpio2 5 GPIO_ACTIVE_HIGH>;
+            status = "okay";
+            bidir;
+        };
+
+    };
+
+    leds {
+        compatible = "gpio-leds";
+
+        status-red {
+            gpios = <&gpio1 18 GPIO_ACTIVE_HIGH>;
+            default-state = "off";
+        };
+
+        status-green {
+            gpios = <&gpio1 20 GPIO_ACTIVE_LOW>;
+            default-state = "on";
+            linux,default-trigger = "timer";
+        };
+
+        status-blue {
+            gpios = <&gpio1 16 GPIO_ACTIVE_LOW>;
+            default-state = "off";
+        };
+    };
+
+    sound {
+        compatible = "fsl,lm-imx-audio-wm8960";
+        model = "wm8960-audio";
+        ssi-controller = <&ssi2>;
+        audio-codec = <&codec>;
+        audio-routing =
+            "Headphone Jack", "HP_L",
+            "Headphone Jack", "HP_R",
+            "Ext Spk", "SPK_LP",
+            "Ext Spk", "SPK_LN",
+            "LINPUT1", "AMIC";
+        ncpins = "LINPUT2", "LINPUT3", "RINPUT1", "RINPUT2", "RINPUT3", "SPK_RP", "SPK_RN", "OUT3";
+        mux-int-port = <2>;
+        mux-ext-port = <3>;
+        iphone-jack;
+        speaker-auto-switch;
+        hp-det-gpios = <&gpio1 9 GPIO_ACTIVE_HIGH>;
+        mic-det-gpios = <&gpio1 6 GPIO_ACTIVE_HIGH>;
+    };
+
+    simpad-plus-psy {
+		status = "okay";
+	compatible = "datarespons,simpad-plus-psy";
+	startup-gpios = <&gpio5 16 GPIO_ACTIVE_HIGH &gpio5 17 GPIO_ACTIVE_HIGH>;
+	bat-ce-gpios = <&gpio4 14 GPIO_ACTIVE_HIGH &gpio3 23 GPIO_ACTIVE_LOW>;
+	dcin-gpio = <&gpio4 5 GPIO_ACTIVE_HIGH>;
+    };
+};
+
+
+&ecspi1 {
+    fsl,spi-num-chipselects = <1>;
+    cs-gpios = <&gpio3 19 0>;
+    pinctrl-names = "default";
+    pinctrl-0 = <&pinctrl_ecspi1>;
+    status = "okay";
+    nodma;
+
+    flash: m25p80@0 {
+        #address-cells = <1>;
+        #size-cells = <1>;
+        compatible = "mx25l3205d", "jedec,spi-nor";
+        spi-max-frequency = <20000000>;
+        reg = <0>;
+        partition@0 {
+            label = "user";
+            reg = <0 0x00200000>;
+        };
+        partition@0x00200000 {
+            label = "factory";
+            reg = <0x00200000 0x00200000>;
+        };
+    };
+};
+
+
+&ecspi2 {
+    fsl,spi-num-chipselects = <1>;
+    cs-gpios = <&gpio5 12 0>;
+    pinctrl-names = "default";
+    pinctrl-0 = <&pinctrl_ecspi2>;
+    status = "disabled";
+};
+
+&fec {
+    pinctrl-names = "default";
+    pinctrl-0 = <&pinctrl_enet_lm>;
+    phy-mode = "rgmii";
+    status = "okay";
+    phy-handle = <&arphy>;
+    phy-supply = <&vgen6_reg>;
+    mdio {
+       #address-cells = <1>;
+       #size-cells = <0>;
+       arphy: ethernet-phy@0 {
+           compatible = "ethernet-phy-ieee802.3-c22";
+           reg = <0x0>;
+           max-speed = <100>;
+           interrupt-parent = <&gpio1>;
+           interrupts = <26 IRQ_TYPE_EDGE_FALLING>;
+       };
+    };
+};
+
+
+&gpc {
+    fsl,ldo-bypass = <0>; /* use ldo-bypass, u-boot will check it and configure */
+    fsl,wdog-reset = <1>; /* watchdog select of reset source */
+};
+
+&wdog1 {
+    status = "disabled";
+};
+
+&wdog2 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_wdog>;
+	fsl,ext-reset-output;
+	status = "okay";
+};
+
+&i2c4 {
+    clock-frequency = <100000>;
+    pinctrl-names = "default";
+    pinctrl-0 = <&pinctrl_i2c4>;
+    status = "okay";
+
+        pmic: pfuze100@8 {
+        compatible = "fsl,pfuze100";
+        reg = <0x08>;
+
+        regulators {
+            sw1a_reg: sw1ab {
+                regulator-min-microvolt = <300000>;
+                regulator-max-microvolt = <1875000>;
+                regulator-boot-on;
+                regulator-always-on;
+                regulator-ramp-delay = <6250>;
+            };
+
+            sw1c_reg: sw1c {
+                regulator-min-microvolt = <300000>;
+                regulator-max-microvolt = <1875000>;
+                regulator-boot-on;
+                regulator-always-on;
+                regulator-ramp-delay = <6250>;
+            };
+
+            sw2_reg: sw2 {
+                regulator-min-microvolt = <800000>;
+                regulator-max-microvolt = <3300000>;
+                regulator-boot-on;
+                regulator-always-on;
+            };
+
+            sw3a_reg: sw3a {
+                regulator-min-microvolt = <400000>;
+                regulator-max-microvolt = <1975000>;
+                regulator-boot-on;
+                regulator-always-on;
+            };
+
+            sw3b_reg: sw3b {
+                regulator-min-microvolt = <400000>;
+                regulator-max-microvolt = <1975000>;
+                regulator-boot-on;
+                regulator-always-on;
+            };
+
+            sw4_reg: sw4 {
+                regulator-min-microvolt = <800000>;
+                regulator-max-microvolt = <3300000>;
+                regulator-boot-on;
+                regulator-always-on;
+            };
+
+            swbst_reg: swbst {
+                regulator-min-microvolt = <5000000>;
+                regulator-max-microvolt = <5150000>;
+                regulator-boot-on;
+                regulator-always-on;
+            };
+
+            snvs_reg: vsnvs {
+                regulator-min-microvolt = <1000000>;
+                regulator-max-microvolt = <3000000>;
+                regulator-boot-on;
+                regulator-always-on;
+            };
+
+            vref_reg: vrefddr {
+                regulator-boot-on;
+                regulator-always-on;
+            };
+
+            vgen1_reg: vgen1 {
+                regulator-min-microvolt = <800000>;
+                regulator-max-microvolt = <1550000>;
+            };
+
+            vgen2_reg: vgen2 {
+                regulator-min-microvolt = <800000>;
+                regulator-max-microvolt = <1550000>;
+            };
+
+            vgen3_reg: vgen3 {
+                regulator-min-microvolt = <1800000>;
+                regulator-max-microvolt = <3300000>;
+            };
+
+            vgen4_reg: vgen4 {
+                regulator-min-microvolt = <1800000>;
+                regulator-max-microvolt = <3300000>;
+                regulator-always-on;
+            };
+
+            vgen5_reg: vgen5 {
+                regulator-min-microvolt = <1800000>;
+                regulator-max-microvolt = <3300000>;
+                regulator-always-on;
+            };
+
+            vgen6_reg: vgen6 {
+                regulator-min-microvolt = <1800000>;
+                regulator-max-microvolt = <3300000>;
+                regulator-always-on;
+            };
+        };
+    };
+
+};
+
+&i2c1 {
+    clock-frequency = <100000>;
+    pinctrl-names = "default";
+    pinctrl-0 = <&pinctrl_i2c1>;
+    status = "okay";
+
+    codec: wm8960@1a {
+        compatible = "wlf,wm8960-lm";
+        reg = <0x1a>;
+        clocks = <&clks IMX6QDL_CLK_CKO>;
+        clock-names = "mclk";
+        wlf,shared-lrclk;
+       };
+
+    mma8451@1c {
+        compatible = "fsl,mma8451";
+        reg = <0x1c>;
+        position = <0>;
+        vdd-supply = <&sw2_reg>;
+        vddio-supply = <&vgen4_reg>;
+        interrupt-parent = <&gpio2>;
+        interrupts = <1 8>;
+        interrupt-route = <1>;
+    };
+
+};
+
+&i2c3 {
+    clock-frequency = <100000>;
+    pinctrl-names = "default";
+    pinctrl-0 = <&pinctrl_i2c3>;
+    status = "okay";
+
+    egalax_ts@4 {
+        compatible = "eeti,egalax_ts";
+        reg = <0x4>;
+        interrupt-parent = <&gpio7>;
+        interrupts = <13 2>;
+        wakeup-gpios = <&gpio7 13 0>;
+        disable-suspend = <1>;
+        };
+
+    bq27x00-battery@55 {
+        compatible = "ti,bq27500";
+        reg = <0x55>;
+        status = "okay";
+    };
+
+	main_rtc: rtc-m41t82@68 {
+		compatible = "ds,m41t82";
+		reg = <0x68>;
+		xtal-cap = /bits/ 8 <0xc8>;		/* Fix for HW bug, turn off capacitance */
+	};
+
+};
+
+
+&iomuxc {
+    pinctrl-names = "default";
+    pinctrl-0 = <&pinctrl_hog_1>;
+
+    hog {
+        pinctrl_hog_1: hoggrp-1 {
+            fsl,pins = <
+                MX6QDL_PAD_NANDF_CLE__GPIO6_IO07    0x80000000      /* GP--BT_HOST_WAKE */
+                MX6QDL_PAD_GPIO_4__GPIO1_IO04       0x0001b000      /* Function key (active low, pullup) */
+                MX6QDL_PAD_KEY_COL2__GPIO4_IO10     0x80000000      /* Touch reset (low)    */
+                MX6QDL_PAD_KEY_ROW1__GPIO4_IO09     0x80000000      /* AUX_5V_EN */
+                MX6QDL_PAD_KEY_ROW4__GPIO4_IO15     0x00002051      /* CLR_STATUS */
+                MX6QDL_PAD_KEY_COL4__GPIO4_IO14     0x1b0b1         /* GP--Charger_nCE */
+                MX6QDL_PAD_GPIO_0__CCM_CLKO1        0x130b0         /* External audio clk */
+                MX6QDL_PAD_GPIO_6__GPIO1_IO06       0x0001b040		/* Mic detect */
+                MX6QDL_PAD_GPIO_7__GPIO1_IO07       0x80000000      /* LCD flip H */
+                MX6QDL_PAD_GPIO_8__GPIO1_IO08       0x80000000      /* LCD flip V */
+                MX6QDL_PAD_GPIO_9__GPIO1_IO09       0x00010000      /* GP--HP_DET, pullup */
+                MX6QDL_PAD_GPIO_18__GPIO7_IO13      0x0001b8b1      /* GP--CTP_nINT (touch interrupt */
