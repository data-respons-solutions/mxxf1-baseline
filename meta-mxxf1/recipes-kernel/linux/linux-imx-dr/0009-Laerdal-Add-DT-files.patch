diff --git a/arch/arm/boot/dts/laerdal-cpu-module-revC-q.dts b/arch/arm/boot/dts/laerdal-cpu-module-revC-q.dts
new file mode 100644
index 000000000000..5b6cc596b223
--- /dev/null
+++ b/arch/arm/boot/dts/laerdal-cpu-module-revC-q.dts
@@ -0,0 +1,19 @@
